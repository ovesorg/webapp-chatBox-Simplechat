# Application Configuration
REACT_APP_NAME="Omnivoltaic Assist"
REACT_APP_VERSION=1.0.0
REACT_APP_ENV=development

# API Configuration
REACT_APP_API_URL=https://oves-bot.omnivoltaic.com
REACT_APP_WS_URL=wss://oves-bot.omnivoltaic.com/chat

# Azure AD Configuration
# Get these from Azure Portal > App Registrations
REACT_APP_AZURE_CLIENT_ID=your-azure-client-id-here
REACT_APP_AZURE_TENANT_ID=your-azure-tenant-id-here
REACT_APP_AZURE_REDIRECT_URI=http://localhost:3000

# Google OAuth Configuration
# Get this from Google Cloud Console > Credentials
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here

# Feature Flags
REACT_APP_ENABLE_FEEDBACK=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# Server Configuration (for production build)
PORT=4500
NODE_ENV=development
